<div id="weatherWidget">
  {{#each partials.weather.locations}}
  <div id="location">
    <h1>{{name}}</h1>
    <a href="{{forecastUrl}}">
      <img src="{{iconUrl}}" alt="{{weather}}">
      {{weather}}, {{temp}}
    </a>
  </div>
  {{/each}}
  <small>Source: <a href="http://www.wunderground.com">Weather
  Underground</a></small>
</div>
  
